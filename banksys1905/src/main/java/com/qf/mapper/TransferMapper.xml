<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qf.dao.TransferDao">
<!--    名称空间-->
    <insert id="add" parameterType="com.qf.entity.Transfer">
        insert into t_transfer(uid,money,consum_type,create_time,consument,balance)
        value (#{uid} ,#{money},#{consumType},now(),#{conment}.#{balance})
    </insert>

</mapper>